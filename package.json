{
  "name": "wiki-langlinks-web",
  "version": "0.2.0",
  "description": "Web app for multi-language translation via MediaWiki",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "xo",
    "build": "webpack --env=prod",
    "ci": "npm run lint && npm run build"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iredchuk/wiki-langlinks-web.git"
  },
  "keywords": [
    "wiki",
    "mediawiki",
    "languages",
    "translation"
  ],
  "author": "iredchuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iredchuk/wiki-langlinks-web/issues"
  },
  "homepage": "https://github.com/iredchuk/wiki-langlinks-web#readme",
  "dependencies": {
    "prop-types": "~15.5.10",
    "react": "~15.6.1",
    "react-dom": "~15.6.1",
    "react-loading": "~0.1.4",
    "styled-components": "~2.1.0",
    "superagent": "~3.5.2"
  },
  "devDependencies": {
    "babel-core": "~6.25.0",
    "babel-loader": "~7.1.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.5.2",
    "babel-preset-react": "~6.24.1",
    "eslint-config-xo-react": "~0.12.0",
    "eslint-plugin-react": "~7.1.0",
    "webpack": "~3.0.0",
    "webpack-dev-server": "~2.5.0",
    "xo": "~0.18.2"
  },
  "xo": {
    "extends": "xo-react",
    "envs": [
      "node",
      "browser"
    ],
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  }
}
